{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "semicolon": true,
    "quotemark": true,
    "object-literal-sort-keys": false,
    "no-empty-interface": false
  }
}
